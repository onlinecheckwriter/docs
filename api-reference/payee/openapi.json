{
  "openapi": "3.0.1",
  "info": {
    "title": "Online Check Writer API",
    "description": "API documentation for retrieving payees.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://staging.api.ocw.sebipay.com/api/v3",
      "description": "Staging Server"
    }
  ],
  "paths": {
    "/payees": {
      "get": {
        "summary": "Retrieve all payees",
        "description": "List all payee records.",
        "parameters": [
          {
            "name": "perPage",
            "in": "query",
            "description": "Number of records that you want to receive in the response. By default, 10 records will be listed.",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 10
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "For navigating through pages. By default, the first page will be listed.",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "term",
            "in": "query",
            "description": "Filter records using any keywords.",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with a list of payees.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "meta": {
                          "type": "object",
                          "properties": {
                            "total": { "type": "integer", "example": 9971 },
                            "currentPage": { "type": "integer", "example": 1 },
                            "perPage": { "type": "integer", "example": 10 }
                          }
                        },
                        "payees": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "payeeId": { "type": "string" },
                              "name": { "type": "string" },
                              "nickName": { "type": "string" },
                              "company": { "type": "string" },
                              "email": { "type": "string" },
                              "phone": { "type": "string" },
                              "address1": { "type": "string" },
                              "address2": { "type": "string" },
                              "city": { "type": "string" },
                              "state": { "type": "string" },
                              "zip": { "type": "string" },
                              "country": { "type": "string" }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "exampleResponse": {
                    "value": {
                      "success": true,
                      "data": {
                        "meta": {
                          "total": 99,
                          "currentPage": 1,
                          "perPage": 2
                        },
                        "payees": [
                          {
                            "payeeId": "RJ6qGAAxJPGE1rD",
                            "name": "Payee Name 1",
                            "nickName": "Payee Nick Name 1",
                            "company": "",
                            "email": "test@test.com",
                            "phone": "",
                            "address1": "address1",
                            "address2": "address2",
                            "city": "city",
                            "state": "state",
                            "zip": "123456",
                            "country": "US"
                          },
                          {
                            "payeeId": "dJM5Gbk3LQeY329",
                            "name": "Payee Name 2",
                            "nickName": "Payee Nick Name 2",
                            "company": "",
                            "email": "test@test.com",
                            "phone": "",
                            "address1": "address1",
                            "address2": "address2",
                            "city": "city",
                            "state": "state",
                            "zip": "123456",
                            "country": "US"
                          }
                        ]
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized. Missing or invalid token.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": { "type": "string", "example": "Unauthenticated." }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": ["Payees"]
      }
    },
    "/payees/{payeeId}": {
      "get": {
        "summary": "Retrieve a payee",
        "description": "Details of an existing payee.",
        "parameters": [
          {
            "name": "payeeId",
            "in": "path",
            "description": "The ID of the payee to retrieve.",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with the details of a payee.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "payeeId": { "type": "string" },
                        "name": { "type": "string" },
                        "nickName": { "type": "string" },
                        "company": { "type": "string" },
                        "email": { "type": "string" },
                        "phone": { "type": "string" },
                        "address1": { "type": "string" },
                        "address2": { "type": "string", "nullable": true },
                        "city": { "type": "string" },
                        "state": { "type": "string" },
                        "zip": { "type": "string" },
                        "country": { "type": "string" }
                      }
                    }
                  }
                },
                "examples": {
                  "exampleResponse": {
                    "value": {
                      "success": true,
                      "data": {
                        "payeeId": "RJ6qGAAxJPGE1rD",
                        "name": "John Doe",
                        "nickName": "JD",
                        "company": "John's Services",
                        "email": "john.doe@example.com",
                        "phone": "+1234567890",
                        "address1": "123 Main St",
                        "address2": "Apt 4B",
                        "city": "Springfield",
                        "state": "IL",
                        "zip": "62704",
                        "country": "US"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Payee not found.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean", "example": false },
                    "errorMsg": { "type": "string", "example": "Payee not found." }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized. Missing or invalid token.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": { "type": "string", "example": "Unauthenticated." }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": ["Payees"]
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}