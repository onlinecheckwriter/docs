                              "city": { "type": "string" },
                              "state": { "type": "string" },
                              "zip": { "type": "string" },
                              "phone": { "type": "string", "nullable": true },
                              "walletId": { "type": "string" },
                              "createdDate": { "type": "string", "format": "date" }
                            }
                          }
